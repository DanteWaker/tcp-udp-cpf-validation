{
  "name": "cpf-validator",
  "version": "1.0.0",
  "description": "Sistema cliente-servidor para validação de CPF",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "tcp-server": "node --experimental-specifier-resolution=node dist/tcp-server.js",
    "tcp-client": "node --experimental-specifier-resolution=node dist/tcp-client.js",
    "udp-server": "node --experimental-specifier-resolution=node dist/udp-server.js",
    "udp-client": "node --experimental-specifier-resolution=node dist/udp-client.js"
  },
  "keywords": ["cpf", "validator", "tcp", "udp", "typescript"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 